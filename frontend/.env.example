# Backend API Base URL
VITE_API_BASE_URL=http://localhost:8000

# External API Configuration
VITE_DISDAGKOPUKM_API_URL=https://disdagkopukm.wonosobokab.go.id/api

# Google Maps API Configuration
# Dapatkan API key dari: https://console.cloud.google.com/google/maps-apis
# Aktifkan: Elevation API, Maps JavaScript API, Geocoding API
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Auto-refresh interval (in milliseconds)
# Default: 300000 (5 minutes)
VITE_MARKET_REFRESH_INTERVAL=300000

# Enable auto-sync to database
VITE_AUTO_SYNC_ENABLED=true
